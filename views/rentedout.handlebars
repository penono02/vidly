<Doctype html>
<html>
<head>
  <title>Rented</title>

</head>

<body>
  <h1>Movie Rented</h1>
<p>
  <a href="/api/rented/">Refresh database</a>
</p>
<table>

  <th>User ID</th><th>name</th><th>Title</th><th>Email</th><th>Phone </th><th>Daily Rental Rate</th><th>Date out</th>
  {{#each rentedMovies}}
  <tr>
      <td>{{_id}}</td><td>{{user.name}}</td><td>{{movie.title}}</td><td>{{user.email}}</td><td>{{user.phone}}</td><td>{{movie.dailyRentalRate}}</td>
      <td>{{dateOut}}</td>

  </tr>
  {{/each}}

</table><br />

</body>

</html>
